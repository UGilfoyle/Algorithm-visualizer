{
    "version": 2,
    "buildCommand": null,
    "outputDirectory": ".",
    "devCommand": null,
    "installCommand": null,
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/icons/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*\\.(css|js))",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=2592000"
                }
            ]
        },
        {
            "source": "/index.html",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }
            ]
        }
    ],
    "functions": {
        "api/visitors.js": {
            "maxDuration": 10
        }
    }
}